{
  "_version": "1.76.0",
  "sap.app": {
    "id": "mdq.re",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "f2529db2-1661-402c-8287-e246252ad33b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/rules/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "businessPartnerService": {
        "uri": "/odata/v4/business-partner/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "Rules-Manage": {
          "semanticObject": "Rules",
          "action": "Manage",
          "title": "{{Rules-Manage.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.comp": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "mdq.re.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "bpModel": {
        "dataSource": "businessPartnerService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "mdq.re.view",
        "async": true,
        "viewPath": "mdq.re.view"
      },
      "routes": [
        {
          "name": "List",
          "pattern": "",
          "target": ["List"]
        },
        {
          "name": "Rule",
          "pattern": "Rule/{ruleId}",
          "target": ["Rule"]
        },
        {
          "name": "Create",
          "pattern": "create",
          "target": ["Create"]
        }
      ],
      "targets": {
        "List": {
          "id": "List",
          "name": "List"
        },
        "Rule": {
          "id": "Rule",
          "name": "Rule"
        },
        "Create": {
          "id": "Create",
          "name": "Create"
        }
      }
    },
    "rootView": {
      "viewName": "mdq.re.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
